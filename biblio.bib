journal = {Cities},
keywords = {Land cover,Land-use,Morogoro,Spatio-temporal pattern,Urban expansion},
number = {June},
pages = {102876},
publisher = {Elsevier},
title = {{On the absurdity of rapid urbanization: Spatio-temporal analysis of land-use changes in Morogoro, Tanzania}},
url = {https://doi.org/10.1016/j.cities.2020.102876},
volume = {107},
